<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".feature.vistaMenuOpcionesMedicas.MenuOpcionesMedicasView">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <ImageButton
            android:id="@+id/btn_opcionResultados"
            android:layout_width="351dp"
            android:layout_height="172dp"
            android:layout_marginTop="152dp"
            android:src="@drawable/boton_opcion_resultados_puebas"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:contentDescription="0"/>

        <ImageButton
            android:id="@+id/btn_opcionAlimentacion"
            android:layout_width="351dp"
            android:layout_height="172dp"
            android:layout_marginTop="19dp"
            android:src="@drawable/boton_opcion_alimentacion"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_opcionResultados"
            android:contentDescription="0"/>

        <ImageButton
            android:id="@+id/btn_opcionEnergia"
            android:layout_width="351dp"
            android:layout_height="172dp"
            android:layout_marginTop="28dp"
            android:src="@drawable/boton_opcion_resultados_energia"
            android:background="@color/white"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_opcionAlimentacion"
            android:contentDescription="0"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>