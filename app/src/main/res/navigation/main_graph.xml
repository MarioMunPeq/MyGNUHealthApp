<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/nuevoRegistroServidorView">
    <fragment
        android:id="@+id/inicioView"
        android:name="com.example.proyectohospitalgambia.feature.vistaInicio.InicioView"
        android:label="fragment_inicio_view"
        tools:layout="@layout/fragment_inicio_view" >
        <action
