        android:name="com.example.proyectohospitalgambia.feature.vistaAjustesConexion.AjustesConexionView"
        android:label="fragment_ajustes_conexion_view"
        tools:layout="@layout/fragment_ajustes_conexion_view" />
    <fragment
        android:id="@+id/profileView"
        android:name="com.example.proyectohospitalgambia.feature.vistaProfile.ProfileView"
